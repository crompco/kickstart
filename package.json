    "vue-template-compiler": "^2.2.6",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "axios": "^0.15.3",
    "nuxt": "^2.15.2",
    "popper.js": "^1.9.1",
